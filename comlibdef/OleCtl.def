(* OCPFIPARAMS structure - parameters for OleCreatePropertyFrameIndirect *)
TYPE OCPFIPARAMS= RECORD
                    cbStructSize:          WIN32.ULONG;
                    hWndOwner:             WIN32.HWND;
                    x, y:                  INTEGER;
                    lpszCaption:           Ole2.LPCOLESTR;
                    cObjects:              WIN32.ULONG;
                    lplpUnk:               ARRAY [0..0] OF Ole2.IUnknown;
                    cPages:                WIN32.ULONG;
                    lpPages:               ARRAY [0..0] OF WIN32.CLSID;
                    lcid:                  WIN32.LCID;
                    dispidInitialProperty: Ole2.DISPID;
                  END; (* record *)


(* FONTDESC structure *)
TYPE FONTDESC= RECORD
                 cbSizeofstruct:  WIN32.UINT;
                 lpstrName:       Ole2.LPOLESTR;
                 cySize:          Ole2.CY;
                 sWeight:         WIN32.SHORT;
                 sCharset:        WIN32.SHORT;
                 fItalic:         WIN32.BOOL;
                 fUnderline:      WIN32.BOOL;
                 fStrikethrough:  WIN32.BOOL;
               END; (* record *)

(* PICTDESC structure *)
CONST PICTYPE_UNINITIALIZED= -1;
      PICTYPE_NONE=           0;
      PICTYPE_BITMAP=         1;
      PICTYPE_METAFILE=       2;
      PICTYPE_ICON=           3;
      PICTYPE_ENHMETAFILE=    4;

TYPE PICTDESC= RECORD
                 cbSizeofstruct: WIN32.UINT;
                 picType:        WIN32.UINT;
                 CASE :CARDINAL OF
                   0: hbitmap: WIN32.HBITMAP;
                      hpal:    WIN32.HPALETTE;
                 | 1: hmeta:   WIN32.HMETAFILE;
                      xExt:    INTEGER;
                      yExt:    INTEGER;
                 | 2: hicon:   WIN32.HICON;
                 | 3: hemf:    WIN32.HENHMETAFILE;
                 ELSE
                 END; (* case *)
               END; (* record *)

