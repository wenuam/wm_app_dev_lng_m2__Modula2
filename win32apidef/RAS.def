(* AutoDial DLL function parameter block.*)
TYPE
    RASADPARAMS = RECORD
    dwSize      : DWORD;
    hwndOwner   : HWND;
    dwFlags     : DWORD;
    xDlg        : LONG;
    yDlg        : LONG;
    END;
    LPRASADPARAMS = POINTER TO RASADPARAMS;

(* AutoDial DLL function parameter block "dwFlags." *)
CONST
    RASADFLG_PositionDlg            =00000001h;

(* Prototype AutoDial DLL function.*)
