PROCEDURE MAKEINTRESOURCEA(i : INTEGER) : LPSTR; MACRO;
BEGIN
    RETURN CAST(LPSTR, MAKEADR(i));
END MAKEINTRESOURCEA;

PROCEDURE MAKEINTRESOURCEW(i : INTEGER) : LPWSTR; MACRO;
BEGIN
