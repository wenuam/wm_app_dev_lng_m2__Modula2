		RECORD
			uMsg        : DWORD;
			wParamL     : WORD;
			wParamH     : WORD;
		END;
    PHARDWAREINPUT = POINTER TO HARDWAREINPUT;
    LPHARDWAREINPUT = PHARDWAREINPUT;

    (*NT only*)
    INPUT =
		RECORD
			type        : DWORD;

			CASE : CARDINAL OF
				0: mi : MOUSEINPUT;|
				1: ki : KEYBDINPUT;|
				2: hi : HARDWAREINPUT;
			ELSE
			END;
		END;
