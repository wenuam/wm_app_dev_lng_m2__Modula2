(* This returns a bitmap, which allows us to    *)
(* handle the higher-valued function numbers.   *)
(* Use  SQL_FUNC_EXISTS(bitmap,function_number) *)
(* to determine if the function exists.         *)
(*----------------------------------------------*)

    SQL_API_ODBC3_ALL_FUNCTIONS      = 999;
    SQL_API_ODBC3_ALL_FUNCTIONS_SIZE = 250;     (* array of 250 words *)

PROCEDURE SQL_FUNC_EXISTS (VAR pfExists : ARRAY OF SQLUSMALLINT; uwAPI : SQLUSMALLINT) : SQLUSMALLINT; MACRO;
BEGIN
    IF (ORD(pfExists [uwAPI SHR 4]) BAND (ORD(1) SHL (uwAPI BAND 000FH))) # 0 THEN RETURN SQL_TRUE ELSE RETURN SQL_FALSE END;
END SQL_FUNC_EXISTS;

