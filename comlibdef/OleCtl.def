(* Property frame APIs *)

PROCEDURE OleCreatePropertyFrame(hwndOwner:   WIN32.HWND;
                                 x, y:        WIN32.UINT;
                                 lpszCaption: Ole2.LPCOLESTR;
                                 cObjects:    WIN32.ULONG;
                                 ppUnk:       ARRAY OF Ole2.IUnknown;
                                 cPages:      WIN32.ULONG;
                                 pPageClsID:  ARRAY OF WIN32.LPCLSID;
                                 lcid:        WIN32.LCID;
                                 dwReserved:  WIN32.DWORD;
                                 pvReserved:  WIN32.LPVOID): WIN32.HRESULT;

PROCEDURE OleCreatePropertyFrameIndirect(lpParams: OCPFIPARAMS): WIN32.HRESULT;


(* Standard type APIs *)

PROCEDURE OleTranslateColor(clr:        OleC.OLE_COLOR;
                            hpal:       WIN32.HPALETTE;
                            lpcolorref: WIN32.LPCOLORREF): WIN32.HRESULT;

PROCEDURE OleCreateFontIndirect(    FontDesc: FONTDESC;
                                    iid:      WIN32.IID;
                                VAR iUnknown: Ole2.IUnknown): WIN32.HRESULT;

PROCEDURE OleCreatePictureIndirect(    PictDesc: PICTDESC;
                                       iid:      WIN32.IID;
                                       fOwn:     WIN32.BOOL;
                                   VAR iUnknown: Ole2.IUnknown): WIN32.HRESULT;

PROCEDURE OleLoadPicture(    iStream:  Ole2.IStream;
                             lSize:    WIN32.LONG;
                             fRunmode: WIN32.BOOL;
                             iid:      WIN32.IID;
                         VAR iUnknown: Ole2.IUnknown): WIN32.HRESULT;

PROCEDURE OleLoadPictureEx(    iStream:      Ole2.IStream;
                               lSize:        WIN32.LONG;
                               fRunmode:     WIN32.BOOL;
                               iid:          WIN32.IID;
                               xSizeDesired: WIN32.DWORD;
                               ySizeDesired: WIN32.DWORD;
                               dwFlags:      WIN32.DWORD;
                           VAR iUnknown:     Ole2.IUnknown): WIN32.HRESULT;

PROCEDURE OleLoadPicturePath(    szURLorPath:    Ole2.LPOLESTR;
                                 iUnknownCaller: Ole2.IUnknown;
                                 dwReserved:     WIN32.DWORD;
                                 clrReserved:    OleC.OLE_COLOR;
