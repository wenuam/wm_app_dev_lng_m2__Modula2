PROCEDURE DeleteUrlCacheEntryA (lpszUrlName : ARRAY OF ACHAR) : BOOL;

PROCEDURE DeleteUrlCacheEntryW (lpszUrlName : ARRAY OF UCHAR) : BOOL;

%IF UNICODE %THEN

PROCEDURE DeleteUrlCacheEntry = DeleteUrlCacheEntryW;

%ELSE

PROCEDURE DeleteUrlCacheEntry = DeleteUrlCacheEntryA;

%END

