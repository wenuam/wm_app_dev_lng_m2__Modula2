FROM WIN32 IMPORT DWORD, BOOL;

<*/CALLS:WINDOWSCALL/DLLDEF*>
<*/NOHIGH*>
%IF AMD64 %THEN
<*/ALIGN:8/NOPACK*>
%ELSE
<*/ALIGN:4/NOPACK*>
%END

