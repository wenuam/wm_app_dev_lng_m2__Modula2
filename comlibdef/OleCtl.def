                             VAR ppvRet:         WIN32.LPVOID): WIN32.HRESULT;

PROCEDURE OleLoadPictureFile(    varFileName:  Ole2.VARIANT;
                             VAR iDispPicture: Ole2.IUnknown): WIN32.HRESULT;

PROCEDURE OleLoadPictureFileEx(    varFileName:  Ole2.VARIANT;
                                   xSizeDesired: WIN32.DWORD;
                                   ySizeDesired: WIN32.DWORD;
                                   dwFlags:      WIN32.DWORD;
                               VAR iDispPicture: Ole2.IUnknown): WIN32.HRESULT;

PROCEDURE OleSavePictureFile(iDispPicture: Ole2.IUnknown;
                             bstrFileName: Ole2.BSTR): WIN32.HRESULT;

PROCEDURE OleIconToCursor(hinstExe: WIN32.HINSTANCE;
