UNSAFEGUARDED DEFINITION MODULE ConnectionPoint;

IMPORT WIN32;
IMPORT Ole2;

CLASS C;
  REVEAL Connect;

  PROCEDURE Connect(iUnknownSource: Ole2.IUnknown;
                    iUnknownSink:   Ole2.IUnknown;
                    iidSink:        WIN32.IID): BOOLEAN;
  (* connects a source interface on a sink object to a connectable object *)
  (* iUnknownSource = iUnknown of connectable object *)
  (* iUnknownSink   = iUnknown of sink object *)
  (* iidSink        = IID of the sink interface *)
END C;



END ConnectionPoint.
